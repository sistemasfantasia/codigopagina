{
  "store.custom#blog-colchones": {
    "blocks": ["flex-layout.row#contenedor-vista-blog"]
  },
  //CONTENEDOR VISTA BLOG
  "flex-layout.row#contenedor-vista-blog": {
    "children": ["flex-layout.col#contenedor-responsive-vista-blog"],
    "props": {
      "blockClass": ["contenedor-vista-blog"]
    }
  },
  "flex-layout.col#contenedor-responsive-vista-blog": {
    "children": [
      "flex-layout.row#contenedor-miga-pan-vista-blog",
      "list-context.image-list#banner-vista-blog",
      "flex-layout.row#contenedor-cards-vista-blog",
      "flex-layout.col#contenedor-articulos-vista-blog",
      "flex-layout.row#contenedor-vitrina-no-encontrado"
    ],
    "props": {
      "blockClass": ["contenedor-responsive-vista-blog"]
    }
  },
  "responsive-layout.desktop#contenedor-vista-blog": {
    "children": ["flex-layout.col#contenedor-vista-blog"]
  },
  "responsive-layout.tablet#contenedor-vista-blog": {
    "children": ["flex-layout.col#contenedor-vista-blog-mobile"]
  },
  "responsive-layout.phone#contenedor-vista-blog": {
    "children": ["flex-layout.col#contenedor-vista-blog-mobile"]
  },
  "flex-layout.col#contenedor-vista-blog": {
    "children": [
      "flex-layout.row#contenedor-miga-pan-vista-blog",
      "list-context.image-list#banner-vista-blog",
      "flex-layout.row#contenedor-cards-vista-blog",
      "flex-layout.col#contenedor-articulos-vista-blog",
      "flex-layout.row#contenedor-vitrina-no-encontrado"
    ],
    "props": {
      "blockClass": ["col-contenedor-vista-blog"]
    }
  },

  // MIGA DE PAN VISTA TERMINOS LEGALES
  "flex-layout.row#contenedor-miga-pan-vista-blog": {
    "children": [
      "rich-text#miga-pan-1-vista-blog",
      "image#flecha-miga-pan",
      "rich-text#miga-pan-2-vista-blog"
    ],
    "props": {
      "blockClass": ["contenedor-miga-pan-formulario-contacto"]
    }
  },
  "rich-text#miga-pan-1-vista-blog": {
    "props": {
      "blockClass": ["miga-pan-1"],
      "text": "Miga de pan desktop"
    }
  },
  "rich-text#miga-pan-2-vista-blog": {
    "props": {
      "blockClass": ["miga-pan-2"],
      "text": "Miga de pan desktop"
    }
  },
  // BANNER VISTA HOTELERA
  "list-context.image-list#banner-vista-blog": {
    "children": ["slider-layout#slider-banner-vista-blog"],
    "props": {
      "preload": true,
      "images": [
        {
          "image": "/arquivos/banner-blog.png",
          "mobileImage": "/arquivos/banner-blog-mobile.png"
        }
      ]
    }
  },
  "slider-layout#slider-banner-vista-blog": {
    "props": {
      "itemsPerPage": {
        "desktop": 1,
        "tablet": 1,
        "phone": 1
      },
      "fullWidth": true,
      "showNavigationArrows": "never",
      "showPaginationDots": "never",
      "blockClass": "slider-banner-vista-blog"
    }
  },

