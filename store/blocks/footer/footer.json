    "children": [
      "rich-text#texto-1-seccion-2-footer-2-desktop","rich-text#texto-2-seccion-2-footer-2-desktop"
    ],
    "props": {
      "blockClass": "seccion-2-footer-2-desktop"
    }
  },
  "rich-text#texto-1-seccion-2-footer-2-desktop": {
    "props": {
      "text": "Servicio al cliente",
      "blockClass": "texto-1-seccion-2-footer-2-desktop"
    }
  },
  "rich-text#texto-2-seccion-2-footer-2-desktop": {
    "props": {
      "text": "**Línea de atención gratuita:** 01-8000-423507 **Linea de atención Medellín:** (574) 4444482",
      "blockClass": "texto-2-seccion-2-footer-2-desktop"
    }
  },



  "flex-layout.row#footer-3-desktop": {
    "children": [
      "rich-text#texto-derechos-footer",
      "rich-text#texto-xtrategik",
      "image#logo-xtrategik-footer",
      "rich-text#texto-tecnologia",
      "image#logo-vtex-footer"
    ],
    "props": {
      "blockClass": "footer-3-credits"
    }
  },
  "rich-text#footer": {
    "props": {
      "text": "All stock and product photos are from photos.icons8.com",
      "blockClass": "footer"
    }
  },
  "rich-text#texto-derechos-footer": {
    "props": {
      "text": "©   Copyright 2023 Industrias Fantasía S.A.S. - Todos los derechos reservados",
      "blockClass": ["texto-derechos-footer"],
      "textAlignment": "LEFT",
      "textPosition": "LEFT"
    }
  },
  "rich-text#texto-xtrategik": {
    "props": {
      "text": "Agencia Ecommerce Xtrategik SAS",
      "blockClass": "texto-xtrategik",
      "textAlignment": "CENTER",
      "textPosition": "CENTER"
    }
  },
  "image#logo-xtrategik-footer": {
    "props": {
      "src": "assets/image/logo-xtrategik-footer.svg",
      "alt": "Agencia Ecommerce Xtrategik SAS",
      "width": "70px",
      "blockClass": "logo-xtrategik-footer",
      "link": {
        "url": "https://www.xtrategik.com",
        "openNewTab": true,
        "title": "Xtrategik Agencia Ecommerce"
      }
    }
  },
  "rich-text#texto-tecnologia": {
    "props": {
      "text": "Tecnología ",
      "blockClass": "texto-tecnologia",
      "textAlignment": "CENTER",
      "textPosition": "CENTER"
    }
  },
  "image#logo-vtex-footer": {
    "props": {
      "src": "assets/image/logo-vtex-footer.svg",
      "alt": "Tecnología VTEX",
      "width": "40px",
      "blockClass": "logo-vtex-footer",
      "link": {
        "url": "https://www.vtex.com",
        "openNewTab": true,
        "title": "Tecnología VTEX"
      }
    }
  },

  "footer-layout.mobile": {
    "children": [

      "flex-layout.row#contenedor-footer-mobile",
      "flex-layout.row#footer-xtrategik-mobile"
    ]
  },
  "flex-layout.row#contenedor-footer-mobile": {
    "children": [
      "flex-layout.col#contenedor-footer-mobile"
    ],
    "props":{
      "blockClass":"contenedor-footer-mobile"
    }
  },
  "flex-layout.col#contenedor-footer-mobile": {
    "children": [
      "flex-layout.col#seccion-1-bloque-13-home",
      "menu-item#menu-seccion-creditos-ventas-mobile",
      "menu-item#menu-seccion-enlaces-interes-mobile",
      "flex-layout.row#seccion-1-footer-2-desktop",
      "flex-layout.row#seccion-1-footer-1-desktop",
      "flex-layout.row#seccion-2-footer-1-desktop",
      "image#ventas-online",
      "menu-item#menu-seccion-servicio-cliente-footer-mobile"
    ],
    "props":{
      "blockClass":"col-contenedor-footer-mobile"
    }
  },
  "image#ventas-online": {
    "props": {
      "src": "assets/image/boton-ventas-online.png",
      "alt": "Boton ventas online",
      "blockClass": "boton-ventas-online",
      "link": {
        "url": "",
        "openNewTab": true,
        "title": "WhatsApp ventas online"
      }
    }
  },
  "menu-item#menu-seccion-servicio-cliente-footer-mobile": {
    "props": {
      "id": "menu-seccion-servicio-cliente-footer-mobile",
      "blockClass": ["menus-footer-mobile"],
      "type": "custom",
      "highlight": false,
      "itemProps": {
        "type": "internal",
        "href": "#",
        "noFollow": true,
        "tagTitle": "",
        "text": "Servicio al cliente"
      }
    },
    "blocks": [
      "vtex.menu@2.x:submenu.accordion#menu-seccion-servicio-cliente-footer-mobile"
    ]
  },
  "vtex.menu@2.x:submenu.accordion#menu-seccion-servicio-cliente-footer-mobile": {
    "children": [
      "rich-text#texto-2-seccion-2-footer-2-desktop"
    ],
    "props": {
      "blockClass": "submenu-seccion-servicio-cliente-footer-mobile"
    }
  },

  "flex-layout.row#footer-xtrategik-mobile": {
    "children": [
      "flex-layout.col#footer-xtrategik-mobile"
    ],
    "props":{
      "blockClass":"footer-xtrategik-mobile"
    }
  },
  "flex-layout.col#footer-xtrategik-mobile": {
    "children": [
      "rich-text#texto-derechos-footer",
      "flex-layout.row#footer-xtrategik-mobile-2",
      "flex-layout.row#footer-xtrategik-mobile-3"
    ],
    "props":{
      "blockClass":"footer-xtrategik-mobile"
    }
  },
  "flex-layout.row#footer-xtrategik-mobile-2": {
    "children": [
      "rich-text#texto-xtrategik",
      "image#logo-xtrategik-footer"
    ],
    "props":{
      "blockClass":"fila-footer-xtrategik-mobile"
    }
  },
  "flex-layout.row#footer-xtrategik-mobile-3": {
    "children": [
      "rich-text#texto-tecnologia",
      "image#logo-vtex-footer"
    ],
    "props":{
      "blockClass":"fila-footer-xtrategik-mobile"
    }
  },
  "flex-layout.row#2-footer-mobile": {
    "children": ["flex-layout.col#footer-1-mobile"],
    "props": {
      "blockClass": "payment-methods",
      "paddingTop": 4,
      "paddingBottom": 4
    }
  },
  "flex-layout.col#footer-1-mobile": {
    "children": [
      "flex-layout.row#seccion-1-footer-2-desktop"
    ],
    "props": {
      "blockClass": "payment-methods",
      "paddingTop": 4,
      "paddingBottom": 4,
      "rowGap": 4
    }
  },
  "flex-layout.row#1-footer-mobile": {
    "props": {
      "paddingTop": 4,
      "paddingBottom": 4
    },
    "children": ["vtex.menu@2.x:menu#footer-mobile"]
  },
  "rich-text#footer-mobile": {
    "props": {
      "text": "All stock and product photos are from photos.icons8.com",
      "blockClass": "footer"
    }
  }
}
