  "flex-layout.row#footer-2-desktop": {
    "children": ["flex-layout.row#seccion-1-footer-2-desktop","flex-layout.row#seccion-2-footer-2-desktop"],
    "props": {
      "blockClass": "footer-2-sello-comunicacion"
    }
  },
  "accepted-payment-methods": {
    "props": {
      "paymentMethods": ["MasterCard", "Visa", "Diners Club"]
    }
  },
  "flex-layout.row#seccion-1-footer-2-desktop": {
    "children": [
      "image#sello-icontec-footer","rich-text#texto-icontec-footer"
    ],
    "props": {
      "blockClass": "seccion-1-footer-2-desktop"
    }
  },
  "image#sello-icontec-footer": {
    "props": {
      "src": "assets/image/sello-icontec-footer.svg",
      "alt": "Sello Icontec",
      "width": "70px",
      "blockClass": "sello-icontec-footer"
    }
  },
